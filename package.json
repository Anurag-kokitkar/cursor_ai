{
  "name": "ai-code-review-assistant",
  "version": "1.0.0",
  "description": "AI-Powered Code Review Assistant - Analyze code quality, detect bugs, and suggest improvements",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install:all": "npm install && cd client && npm install",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "keywords": [
    "code-review",
    "static-analysis",
    "ast-parsing",
    "github-api",
    "ai-assistant",
    "code-quality"
  ],
  "author": "AI Code Review Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "mongoose": "^8.0.3",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "multer": "^1.4.5-lts.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "@octokit/rest": "^20.0.2",
    "esprima": "^4.0.1",
    "@babel/parser": "^7.23.6",
    "@typescript-eslint/parser": "^6.13.2",
    "acorn": "^8.11.2",
    "python-ast": "^0.1.1",
    "java-parser": "^2.0.5",
    "eslint": "^8.55.0",
    "jshint": "^2.13.6",
    "pylint": "^3.0.3",
    "socket.io": "^4.7.4",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/node": "^20.10.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}